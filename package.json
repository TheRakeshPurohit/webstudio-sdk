{
  "name": "@webstudio-is/sdk",
  "version": "0.1.22",
  "description": "Webstudio JavaScript / TypeScript API",
  "main": "dist/webstudio-is-sdk.cjs.js",
  "module": "dist/webstudio-is-sdk.esm.js",
  "author": "Webstudio <github@webstudio.is>",
  "repository": "git@github.com:webstudio-is/webstudio-sdk.git",
  "homepage": "https://webstudio.is",
  "scripts": {
    "build": "yarn build:mdn-data && yarn build:prisma && yarn build:dist",
    "build:mdn-data": "./bin/mdn-data.ts ./src/style",
    "build:prisma": "prisma format && prisma generate",
    "typecheck": "tsc --noEmit",
    "build:dist": "yarn preconstruct build",
    "postinstall": "prisma generate",
    "checks": "yarn typecheck",
    "prepublishOnly": "yarn checks && yarn build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@preconstruct/cli": "^2.1.5",
    "@remix-run/react": "^1.2.3",
    "@remix-run/server-runtime": "^1.2.3",
    "@types/css-tree": "^1.0.7",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "camelcase": "^6.3.0",
    "css-tree": "^2.1.0",
    "esbuild": "^0.14.25",
    "esbuild-node-externals": "^1.4.1",
    "esbuild-node-loader": "^0.8.0",
    "mdn-data": "2.0.23",
    "react": "^17.0.2",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/server-runtime": "^1.2.3",
    "react": "^17.0.2"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@stitches/react": "^1.2.7",
    "immer": "^9.0.12",
    "mitt": "^3.0.0",
    "prisma": "^3.10.0",
    "react-nano-state": "^0.4.0"
  },
  "files": [
    "README.md",
    "dist",
    "!*.test.*",
    "prisma"
  ],
  "license": "MIT",
  "private": false,
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "prisma.ts"
    ]
  }
}
